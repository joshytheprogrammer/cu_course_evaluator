{
  "manifest_version": 2,
  "name": "CU Course Evaluator",
  "version": "1.0",
  "description": "Automatically fill out course evaluation forms on Moodle",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": {
      "48": "./assets/icon.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://moodle.cu.edu.ng/mod/feedback/*"],
      "js": ["./content.js"]
    }
  ]
}
